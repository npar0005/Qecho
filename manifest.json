{
	"name": "Qecho",
	"description": "Watch your lectures with speed!",
	"version":"0.1",
	"manifest_version": 2,

	"browser_action": {
		"default_icon": "icons/qecho_logo-128.png"
	},

	"icons": { 
		"16": "icons/qecho_logo-16.png",
		"48": "icons/qecho_logo-48.png",
	   "128": "icons/qecho_logo-128.png" 
	},

	"content_scripts": [{
    	"js": ["content.js"],
    	"matches": ["https://echo360.org.au/lesson/*", "http://echo360.org.au/lesson/*"]
	}]
}